{
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.rulers": [
    100
  ],
  "editor.cursorStyle": "underline",
  "editor.cursorBlinking": "phase",
  // "editor.fontFamily": "Monoid",
  "editor.fontFamily": "JetBrains Mono",
  // "editor.letterSpacing": 0.1,
  "editor.fontSize": 14,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.snippetSuggestions": "top",
  "[elm]": {
    "editor.snippetSuggestions": "inline",
  },
  "explorer.openEditors.visible": 0,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.shell.osx": "/bin/zsh",
  // "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "window.zoomLevel": 1.1,
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.panel.defaultLocation": "right",
  "window.restoreFullscreen": true,
  "editor.formatOnSave": true,
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "cSpell.enableFiletypes": [
    "EEx",
    "HTML (EEx)",
    "c",
    "clojure",
    "cpp",
    "csharp",
    "elixir",
    "elm",
    "git-commit",
    "git-rebase",
    "go",
    "haskell",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "php",
    "plaintext",
    "python",
    "sql",
    "text",
    "typescript",
    "typescriptreact",
    "xml",
    "yaml",
    "yml"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "dist/**": true
    // "node_modules/**": true
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.tabCompletion": "on",
  "editor.fontLigatures": true,
  "workbench.activityBar.visible": false,
  "vim.useSystemClipboard": true,
  "editor.suggestSelection": "first",
  "workbench.sideBar.location": "left",
  "diffEditor.ignoreTrimWhitespace": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "zenMode.hideLineNumbers": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.menus": {
    "editor": false,
    "editorGroup": {
      "blame": true,
      "compare": true
    },
    "editorTab": false,
    "explorer": false,
    "scmGroup": false,
    "scmItem": false
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "cSpell.showStatus": false,
  "breadcrumbs.enabled": false,
  "workbench.editor.revealIfOpen": true,
  "git-graph.showStatusBarItem": false,
  "window.newWindowDimensions": "inherit",
  "terminal.integrated.cursorBlinking": true,
  "gitlens.views.repositories.files.layout": "tree",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "atlascode.jira.jqlList": [
    {
      "id": "58b199a4-c569-4a17-9e09-40173f42dd9a",
      "enabled": true,
      "name": "My jira.tick42.com Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
      "siteId": "jira.tick42.com",
      "monitor": true
    }
  ],
  "atlascode.jira.statusbar.showLogin": true,
  "atlascode.jira.statusbar.showUser": false,
  "atlascode.jira.statusbar.showProduct": false,
  "atlascode.jira.statusbar.showActiveIssue": true,
  "atlascode.bitbucket.statusbar.showProduct": false,
  "atlascode.bitbucket.statusbar.showUser": false,
  "atlascode.bitbucket.statusbar.showLogin": true,
  "calva.paredit.defaultKeyMap": "original",
  "calva.prettyPrintingOptions": {
    "enabled": true,
    "printEngine": "pprint",
    "width": 120
  },
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "atlascode.jira.enabled": true,
  "atlascode.bitbucket.explorer.enabled": true,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "atlascode.jira.statusbar.enabled": false,
  "atlascode.bitbucket.statusbar.enabled": false,
  "tailwindCSS.emmetCompletions": true,
  "vim.foldfix": true,
  "vim.easymotion": true,
  "atlascode.bitbucket.enabled": true,
}
